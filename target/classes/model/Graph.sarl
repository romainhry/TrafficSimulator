package model

import java.util.List
import java.util.ArrayList

class Graph {
	var nodes : ArrayList<GraphNode>
	def add(g : GraphNode) {
		nodes.add(g)
		//this function only add it to the list of nodes in the graph
		//We also need to relate to an existing node before by the funcion
		// "add" of the class GraphNode c
	}
	def getNodes : ArrayList<GraphNode> {
		return this.nodes
	}
}
